{
  "name": "my-app",
  "version": "0.1.0",
  "homepage": "https://donnaphilwedding.github.io/website",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.59",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.11",
    "postcss": "^8.4.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-modern-drawer": "^1.1.2",
    "react-router-dom": "^6.4.0",
    "react-scripts": "5.0.1",
    "tailwind": "^4.0.0",
    "typescript": "^4.8.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "concurrently \"npm run tailwind:start\" \"react-scripts start\"",
    "tailwind:start": "npx tailwindcss -i ./src/index.css -o ./dist/output.css --watch",
    "tailwind:build": "tailwindcss build -i src/index.css -o public/styles.css",
    "build": "npm run tailwind:build && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.4.0",
    "gh-pages": "^4.0.0",
    "tailwindcss": "^3.1.8"
  }
}
